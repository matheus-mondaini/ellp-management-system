# Backend environment variables
ENVIRONMENT=development
SUPABASE_URL=https://project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=replace-me

# Replace <project-ref> and <password> with values from Supabase > Project Settings > Database
DATABASE_URL=postgresql+psycopg://postgres:<password>@db.<project-ref>.supabase.co:6543/postgres?sslmode=require
# For local Postgres development uncomment the following line instead:
# DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/ellp
JWT_SECRET_KEY=replace-me-access
JWT_REFRESH_SECRET_KEY=replace-me-refresh
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRES_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRES_MINUTES=10080
